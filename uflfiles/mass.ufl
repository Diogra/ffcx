
domain = Domain(triangle)

U = FiniteElement("CG", domain, 1) # 3*1
V = VectorElement("CG", domain, 1) # 3*2
W = TensorElement("CG", domain, 1) # 3*4
S = TensorElement("CG", domain, 1, symmetry=True) # 3*3
M = MixedElement(U,V,W,S) # 3*(1+2+4+3) = 30, 30**2 = 900

v0, v1, v2, v3 = TestFunctions(M)
u0, u1, u2, u3 = TrialFunctions(M)
a = (u0*v0 + inner(u1,v1) + inner(u2,v2) + inner(u3,v3))*dx(domain)

