#
# The bilinear form for a mass matrix.
#
# Compile this form with FFC: ffc massmatrix.form.

name = "MassMatrix"
element = FiniteElement("Lagrange", "triangle", 1)

v = BasisFunction(element)
u = BasisFunction(element)
    
a = v*u*dx
