element = VectorElement("Lagrange", triangle, 2)

v  = TestFunction(element)
u  = TrialFunction(element)
w  = Coefficient(element)
c1 = Coefficient(element)
c2 = Coefficient(element)

a = inner(c1, c2)*inner(grad(w)*v, grad(w)*u)*dx
