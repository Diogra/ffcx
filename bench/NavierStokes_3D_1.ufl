scalar = FiniteElement("Lagrange", tetrahedron, 1)
vector = VectorElement("Lagrange", tetrahedron, 1)

v = TestFunction(vector)
u = TrialFunction(vector)
w = Coefficient(vector)
rho = Coefficient(scalar)

a = rho*inner(v, grad(w)*u)*dx
